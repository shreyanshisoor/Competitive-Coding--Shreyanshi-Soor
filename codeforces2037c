#include <bits/stdc++.h>
using namespace std;
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        if (n == 2 || n == 3) {
            cout << -1 << "\n";
            continue;
        }
        vector<int> perm;
        if (n == 4) {
            perm = {1, 3, 2, 4};
        } else {
            perm.push_back(1);
            perm.push_back(n);
            perm.push_back(n - 1);
            for (int i = 2; i <= n - 2; i++) {
                if (i == n - 1) continue;
                perm.push_back(i);
            }
        }
        for (int i = 0; i < n; i++) {
            cout << perm[i] << " ";
        }
        cout << "\n";
    }
    return 0;
}
