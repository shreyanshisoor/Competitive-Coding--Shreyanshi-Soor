#include <bits/stdc++.h>
using namespace std;
const int MAX_X = 1e6 + 1;
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int n;
    cin >> n;
    vector<int> nums(n);
    int maxVal = 0;
    for (int i = 0; i < n; i++) {
        cin >> nums[i];
        maxVal = max(maxVal, nums[i]);
    }
    vector<int> divisors(MAX_X, 0);
    for (int i = 1; i <= maxVal; i++) {
        for (int j = i; j <= maxVal; j += i) {
            divisors[j]++;
        }
    }
    for (int i = 0; i < n; i++) {
        cout << divisors[nums[i]] << "\n";
    }
    return 0;
}
